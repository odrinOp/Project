<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.MenuCtr">
  <center>
    <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
      <children>
        <Label text="Application Menu">
          <font>
            <Font size="30.0" />
          </font>
        </Label>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="17.0">
          <children>
            <Button maxWidth="113.0" minWidth="113.0" mnemonicParsing="false" onAction="#studentsBtn" prefWidth="113.0" text="STUDENTS" />
            <Button maxWidth="113.0" minWidth="113.0" mnemonicParsing="false" onAction="#homeworkBtn" prefWidth="113.0" text="HOMEWORK" />
            <Button maxWidth="113.0" minWidth="113.0" mnemonicParsing="false" onAction="#gradesBtn" prefWidth="113.0" text="GRADES" />
          </children>
          <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="10.0" />
          </padding>
        </VBox>
      </children>
      <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="50.0" />
      </padding>
      <BorderPane.margin>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
      </BorderPane.margin>
    </VBox>
  </center>
</BorderPane>
